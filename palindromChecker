#include<stdio.h>
#include<stdlib.h>
#include<conio.h>
#include<string.h>
int main()
{
	char string[30];
	int i,l,j;
	
	
	printf("Enter a string:");
	scanf("%s",string);
	l=strlen(string);
	for(i=0,j=l-1;i<l/2;i++,j--)
	{
		if(string[i]!=string[j])
		{
			printf("Not a palindrom");
	
			exit(0);
			
		}
	}
	printf("Palindrom");
}
